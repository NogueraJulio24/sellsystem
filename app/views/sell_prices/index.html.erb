<p id="notice"><%= notice %></p>

<h1>Sell Prices</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Total</th>
      <th>Cant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sell_prices.each do |sell_price| %>
      <tr>
        <td><%= sell_price.price %></td>
        <td><%= sell_price.total %></td>
        <td><%= sell_price.cant %></td>
        <td><%= link_to 'Show', sell_price %></td>
        <td><%= link_to 'Edit', edit_sell_price_path(sell_price) %></td>
        <td><%= link_to 'Destroy', sell_price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sell Price', new_sell_price_path %>
